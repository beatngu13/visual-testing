.topHeightBlank { margin: 0 auto; overflow: hidden; height:350px; }
/* banner */
.top { width: 100%; position: absolute; z-index: 1; overflow: hidden; }
.top .banner { width: 100%; }
.top .banner,
.top .layer { width: 1920px; height: 432px; }
